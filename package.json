  "repository": {
    "type": "git",
    "url": "https://github.com/mick-whats/xza-node"
  },
  "bugs": {
    "url": "https://github.com/mick-whats/xza-node/issues"
  },
  "keywords": [
    "lodash",
    "Useful",
    "functions",
    "mixin"
  ],
  "dependencies": {
    "args2": "^1.2.3",
    "big.js": "^5.1.2",
    "bizday": "^2.0.0",
    "object-sizeof": "^1.2.0",
    "objelity": "^1.1.1",
    "pretty-bytes": "^5.1.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coffeescript": "^2.3.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsdoc": "^3.9.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "nyc": "^11.9.0",
    "prettier": "^1.15.3"
